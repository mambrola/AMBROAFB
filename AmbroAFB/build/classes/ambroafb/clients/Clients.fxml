        <fx:include fx:id="editorPanel" source="/ambroafb/general/editor_panel/EditorPanel.fxml"/>
    </top>
    <center>
        <VBox >
            <children>
                <TextField fx:id="filter" promptText="Filter" />
                <ATableView fx:id="table" onMouseClicked="#clickOnTable"  initializer="ambroafb.clients.Client" styleClass="unicode" GridPane.columnIndex="4" GridPane.columnSpan="4" GridPane.rowIndex="1" />
            </children>
        </VBox>
    </center>
</BorderPane>
