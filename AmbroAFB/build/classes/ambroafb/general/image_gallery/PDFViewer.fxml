<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<HBox alignment="CENTER" prefHeight="350" prefWidth="385" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    
    <children>
        <HBox HBox.hgrow="ALWAYS" alignment="CENTER" >
            <ambroafb.general.image_gallery.MagnifierPane>
               <children>
                   <ImageView fx:id="imageView" fitHeight="330" fitWidth="330.0" preserveRatio="true" />
               </children>
            </ambroafb.general.image_gallery.MagnifierPane>
        </HBox>
        <VBox alignment="CENTER_RIGHT" HBox.hgrow="SOMETIMES">
            <children>
                <Button fx:id="up" text="▲" />
                <Button fx:id="down" text="▼" />
            </children>
        </VBox>
    </children>
</HBox>
