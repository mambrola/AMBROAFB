<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.collections.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import ambro.* ?>
<?import ambroafb.general.editor_panel.*?>


<BorderPane id="formPane" 
            stylesheets="/styles/css/clients.css" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="ambroafb.clients.ClientsController">
    <top>
        <EditorPanel fx:id="panel"
            onDelete="#delete" 
            onEdit="#edit" 
            onView="#view" 
            onNew="#add"
            onNewBySample="#addBySample" 
            onRefresh="#refresh" />
    </top>
    <center>
        <ATableView fx:id="table" initializer="ambroafb.clients.Client" styleClass="unicode" GridPane.columnIndex="4" GridPane.columnSpan="4" GridPane.rowIndex="1" />
    </center>
</BorderPane>
