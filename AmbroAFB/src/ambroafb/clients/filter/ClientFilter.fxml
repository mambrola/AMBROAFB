<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="formPane" 
            prefHeight="400.0" 
            prefWidth="600.0" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="ambroafb.clients.filter.ClientFilterController">
    <HBox styleClass="w2" >
        <children>
            <VBox >
                <Label fx:id="date_biger" text="%open_date %biger" /> 
                <DatePicker fx:id="dateBiger" />
            </VBox>
            <VBox >
                <Label fx:id="date_less" text="%open_date %less" /> 
                <DatePicker fx:id="dateLess" />
            </VBox >
            
        </children>
    </HBox>
    
</AnchorPane>
