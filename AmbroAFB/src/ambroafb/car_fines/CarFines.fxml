<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="1200.0" 
            styleClass="mainFxmlClass" stylesheets="/ambroafb/car_fines/carfines.css" 
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="ambroafb.car_fines.CarFinesController">
    <children>
        <HBox alignment="CENTER_LEFT" layoutX="0.0" layoutY="0.0" spacing="5.0" styleClass="menu-bar" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <fx:include source="/ambroafb/general/exit_button/ExitButton.fxml"/>
                <ToggleButton fx:id="enter" onAction = "#enter">
                    <tooltip>
                        <Tooltip text="%enter" />
                    </tooltip>
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" >
                            <Image url="/images/enter.png"/>
                        </ImageView>
                    </graphic>
                </ToggleButton>
            </children>
            <padding>
                <Insets bottom="3.0" left="10.0" top="3.0" />
            </padding>
        </HBox>
        <ListView fx:id="list" layoutX="2.0" AnchorPane.topAnchor="44"  AnchorPane.bottomAnchor="0" />
    </children>
</AnchorPane>
