<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import ambro.*?>
<?import ambroafb.docs.*?>

<ambroafb.general.DialogPanel stylesheets="/styles/css/currencyratedialog.css" 
                              xmlns:fx="http://javafx.com/fxml/1" 
                              xmlns="http://javafx.com/javafx/8"
                              fx:controller="ambroafb.docs.dialog.DocDialogController" >

    <VBox   fx:id="formPane" styleClass="vContainer" >
        <children>
            <HBox styleClass="one">
                <children>
                    <VBox styleClass="couple" >
                        <children>
                            <Label text="%types" />
                            <DocTypeComboBox fx:id="docTypes" styleClass="alwaysEnable" />
                        </children>
                    </VBox>
                </children>
            </HBox>
            <HBox fx:id="concreteScene" styleClass="one" >
                <children>
                </children>
            </HBox>
            <HBox styleClass="okayCancelPane" alignment="CENTER_RIGHT" >
                <fx:include fx:id="okayCancel" source="/ambroafb/general/okay_cancel/DialogOkayCancel.fxml"/>
            </HBox>
        </children>
    </VBox>
</ambroafb.general.DialogPanel>
