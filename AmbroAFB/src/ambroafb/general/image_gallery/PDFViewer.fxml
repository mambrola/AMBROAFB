<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<HBox   xmlns="http://javafx.com/javafx/8"
        xmlns:fx="http://javafx.com/fxml/1">
    
    <children>
        <ImageView fx:id="imageView" preserveRatio="true" fitWidth="380" fitHeight="200" />
        <VBox alignment="CENTER" >
            <children>
                <Button fx:id="up"   text="▲" onAction="#upAction" />
                <Button fx:id="down" text="▼" onAction="#downAction" />
            </children>
        </VBox>
    </children>
</HBox>
